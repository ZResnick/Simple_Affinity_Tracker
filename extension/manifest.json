{
  "manifest_version": 2,
  "name": "Simple Affinity Tracker",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["https://www.urbanoutfitters.com/shop/*"],
      "js": ["productPage.js"]
    },
    {
      "matches": ["https://www.urbanoutfitters.com/new-arrivals*"],
      "js": ["newArrivals.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["background.js"]
  }
}
